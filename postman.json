{
    "name": "User List",
    "description": "",
    "renders": [
        "application/json",
        "text/html"
    ],
    "parses": [
        "application/json",
        "application/x-www-form-urlencoded",
        "multipart/form-data"
    ],
    "actions": {
        "POST": {
            "username": {
                "type": "string",
                "required": true,
                "read_only": false,
                "label": "Nombre de usuario",
                "help_text": "Requerido. 150 carácteres como máximo. Únicamente letras, dígitos y @/./+/-/_ ",
                "max_length": 150
            },
            "email": {
                "type": "email",
                "required": false,
                "read_only": false,
                "label": "Dirección de correo electrónico",
                "max_length": 254
            },
            "password": {
                "type": "string",
                "required": true,
                "read_only": false,
                "label": "Contraseña",
                "max_length": 128
            }
        }
    }
}